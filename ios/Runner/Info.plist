	<string>Este app gostaria de rastrear sua atividade para fornecer anúncios personalizados e melhorar sua experiência de jogo.</string>
	
	<!-- AdMob/Google Ads Privacy -->
	<key>GADDelayAppMeasurementInit</key>
	<true/>
	
	<!-- App Transport Security -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>googleads.g.doubleclick.net</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
			<key>googlesyndication.com</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
		</dict>
	</dict>
	
	<!-- Privacy and Data Usage -->
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	
	<!-- Accessibility -->
	<key>UIRequiresFullScreen</key>
	<true/>
</dict>
</plist>
